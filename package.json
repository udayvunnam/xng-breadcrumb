  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build xng-breadcrumb --configuration production",
    "test": "nx test",
